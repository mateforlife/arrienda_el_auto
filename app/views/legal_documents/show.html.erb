<p id="notice"><%= notice %></p>

<p>
  <b>Document type:</b>
  <%= @legal_document.document_type %>
</p>

<p>
  <b>Files:</b>
  <% @legal_document.files&.each do |image| %>
    <%= image_tag image %>
  <% end %>
</p>

<p>
  <b>Due date:</b>
  <%= @legal_document.due_date %>
</p>

<p>
  <b>Status:</b>
  <%= @legal_document.status %>
</p>


<%= link_to 'Back', vehicle_legal_documents_path(@legal_document.resource) %>
